    <script>
        document.addEventListener('DOMContentLoaded', function () {

            const videoTrigger = document.getElementById('webthv-video-trigger');
            const videoModal = document.getElementById('webthv-video-modal');
            const videoFrame = document.getElementById('webthv-video-frame');
            const closeModal = document.getElementById('webthv-close-modal');

            // Video URL
            const videoUrl = "https://www.youtube.com/embed/-M7pDfRfAb8?autoplay=1";

            videoTrigger.addEventListener('click', function () {
                videoFrame.src = videoUrl;
                videoModal.style.display = 'flex';
                setTimeout(() => {
                    videoModal.classList.add('active');
                }, 10);

                document.body.style.overflow = 'hidden';
            });

            closeModal.addEventListener('click', function () {
                videoModal.classList.remove('active');

                setTimeout(() => {
                    videoModal.style.display = 'none';
                    videoFrame.src = '';
                }, 300);

                document.body.style.overflow = 'auto';
            });

            videoModal.addEventListener('click', function (e) {
                if (e.target === videoModal) {
                    closeModal.click();
                }
            });
            document.addEventListener('keydown', function (e) {
                if (e.key === 'Escape' && videoModal.style.display === 'flex') {
                    closeModal.click();
                }
            });

        });
    </script>
    <script>
        const images = document.querySelectorAll('.igi-gallery-image');
        const modal = document.getElementById('igi-modal');
        const modalImg = document.getElementById('igi-modal-img');
        const closeBtn = document.querySelector('.igi-close');
        const nextBtn = document.querySelector('.igi-next');
        const prevBtn = document.querySelector('.igi-prev');

        let currentIndex = 0;
        const imgArray = Array.from(images);

        const wrappers = document.querySelectorAll('.igi-image-wrapper');

        wrappers.forEach((wrapper, index) => {
            wrapper.addEventListener('click', () => {
                currentIndex = index;
                modal.style.display = 'block';
                modalImg.src = imgArray[index].src;
            });
        });


        closeBtn.onclick = function () {
            modal.style.display = 'none';
        }

        nextBtn.onclick = function () {
            currentIndex = (currentIndex + 1) % imgArray.length;
            modalImg.src = imgArray[currentIndex].src;
        }

        prevBtn.onclick = function () {
            currentIndex = (currentIndex - 1 + imgArray.length) % imgArray.length;
            modalImg.src = imgArray[currentIndex].src;
        }

        // Optional: close modal on background click
        modal.onclick = function (e) {
            if (e.target === modal) {
                modal.style.display = 'none';
            }
        }
    </script>

