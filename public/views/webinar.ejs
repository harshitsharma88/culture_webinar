    <script>
        const sections = document.querySelectorAll('.section');

        const observer = new IntersectionObserver((entries, observer) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('show');
                    observer.unobserve(entry.target); // Show once only
                }
            });
        }, {
            threshold: 0.5 // Trigger when half of section is visible
        });

        sections.forEach(section => {
            observer.observe(section);
        });


        const header = document.querySelector('.new-pr-header');
        const hamburger = document.querySelector('.new-pr-hamburger');
        const navModal = document.querySelector('.new-pr-nav-modal');
        const dropdownToggle = document.querySelector('.new-pr-dropdown-toggle');
        const plusIcon = document.querySelector('.new-pr-plus-icon');
        const dropdown = document.querySelector('.new-pr-dropdown');

        // Variables for scroll detection
        let lastScrollY = 0;
        let ticking = false;

        // Handle scroll event
        window.addEventListener('scroll', () => {
            lastScrollY = window.scrollY;

            if (!ticking) {
                window.requestAnimationFrame(() => {
                    if (lastScrollY > 50) {
                        if (!header.classList.contains('scrolled')) {
                            header.classList.add('scrolled', 'animate-in');
                        }
                    } else {
                        header.classList.remove('scrolled');
                        // Don't remove animate-in class immediately to prevent flickering
                        setTimeout(() => {
                            if (window.scrollY <= 50) {
                                header.classList.remove('animate-in');
                            }
                        }, 300);
                    }
                    ticking = false;
                });

                ticking = true;
            }
        });

        // Ensure header is visible when nav modal is active
        const ensureHeaderVisibility = () => {
            if (navModal.classList.contains('active')) {
                header.style.transform = 'translateY(0)';
                header.style.opacity = '1';
            } else {
                header.style.transform = '';
                header.style.opacity = '';
            }
        };

        // Toggle navigation modal
        hamburger.addEventListener('click', () => {
            hamburger.classList.toggle('active');
            navModal.classList.toggle('active');

            // Prevent scrolling when modal is open
            if (navModal.classList.contains('active')) {
                document.body.style.overflow = 'hidden';
                // Force header to be visible when modal is open
                header.classList.add('scrolled');
            } else {
                document.body.style.overflow = '';
                // Return header to normal state based on scroll position
                if (window.scrollY <= 50) {
                    header.classList.remove('scrolled');
                }
            }

            // Ensure header visibility
            ensureHeaderVisibility();
        });

        // Toggle dropdown
        dropdownToggle.addEventListener('click', (e) => {
            plusIcon.classList.toggle('active');
            dropdown.classList.toggle('active');
        });
    </script>

    <script>
        function animateCounter(elementId, targetValue, duration) {
            const element = document.getElementById(elementId);
            const startTime = Date.now();
            const startValue = 0;

            function updateCounter() {
                const currentTime = Date.now();
                const elapsed = currentTime - startTime;

                if (elapsed < duration) {
                    const value = Math.floor((elapsed / duration) * targetValue);
                    element.textContent = value;
                    requestAnimationFrame(updateCounter);
                } else {
                    element.textContent = targetValue;
                }
            }

            updateCounter();
        }
        window.onload = function () {
            setTimeout(() => animateCounter('webinar-count', 100, 1500), 100);
            setTimeout(() => animateCounter('attended-count', 5000, 1500), 300);
            setTimeout(() => animateCounter('hours-count', 1200, 1500), 500);
            setTimeout(() => animateCounter('subscribers-count', 1500, 1500), 700);
        };

    </script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Main tabs functionality
        const mainTabs = document.querySelectorAll('.new-web-main-tab');
        const categoryContents = document.querySelectorAll('.new-web-category-content');
        
        mainTabs.forEach(tab => {
            tab.addEventListener('click', function() {
                // Remove active class from all tabs
                mainTabs.forEach(t => t.classList.remove('active'));
                // Add active class to clicked tab
                this.classList.add('active');
                
                // Hide all category contents
                categoryContents.forEach(content => content.classList.add('new-web-hidden'));
                
                // Show the corresponding content
                const targetContent = document.getElementById(this.dataset.category + '-content');
                if (targetContent) {
                    targetContent.classList.remove('new-web-hidden');
                }
            });
        });
        
        // Subcategory functionality for Past and All webinars
        const subCategories = document.querySelectorAll('.new-web-sub-category');
        
        subCategories.forEach(subCat => {
            subCat.addEventListener('click', function() {
                // Get the parent category (past or all)
                const parentCategory = this.dataset.subcategory.split('-')[0];
                
                // Remove active class from all subcategories in this parent
                document.querySelectorAll(`.new-web-sub-category[data-subcategory^="${parentCategory}"]`)
                    .forEach(sc => sc.classList.remove('active'));
                
                // Add active class to clicked subcategory
                this.classList.add('active');
                
                // Hide all subcategory contents in this parent
                document.querySelectorAll(`#${parentCategory}-content .new-web-subcategory-content`)
                    .forEach(content => content.classList.add('new-web-hidden'));
                
                // Show the corresponding subcategory content
                const targetSubContent = document.getElementById(this.dataset.subcategory + '-content');
                if (targetSubContent) {
                    targetSubContent.classList.remove('new-web-hidden');
                }
            });
        });
    });
</script>
